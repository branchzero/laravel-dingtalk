{
    "name": "branchzero/laravel-dingtalk",
    "description": "钉钉 SDK Laravel/Lumen 扩展包（Dingtalk SDK for Laravel/Lumen）",
    "keywords": [
        "laravel",
        "lumen",
        "钉钉",
        "dingtalk",
        "丁丁",
        "dingding",
        "ding"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "陈恺垣",
            "email": "contact@chenky.com"
        },
        {
            "name": "branchzero",
            "email": "branchzero@gmail.com"
        }
    ],
    "require": {
        "branchzero/dingtalk": "^1.0",
        "illuminate/container": "^5.1|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Branchzero\\LaravelDingtalk\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Branchzero\\LaravelDingtalk\\DingtalkServiceProvider"
            ],
            "aliases": {
                "EasyDingtalk": "Branchzero\\LaravelDingtalk\\Facade"
            }
        }
    }
}